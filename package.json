{
  "name": "ntp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p watch:front watch:back",
    "watch:front": "cross-env NODE_ENV=development webpack  --watch",
    "watch:back": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --watch",
    "build": "run-p build:front build:back",
    "build:front": "cross-env NODE_ENV=production webpack ",
    "build:back": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "start": "node dist/index.js",
    "prod": "yarn build && cross-env PORT=3063 yarn start"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cacheable-response": "^2.1.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "localforage": "^1.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^7.1.1",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
